@{
    Layout = "~/Areas/M/Views/Shared/_Layout.cshtml";
}

@section styles{
   @* @Scripts.Render("/Content/datepicker");*@
    <link href="~/Areas/M/Styles/JBCJ_Product1.css" rel="stylesheet" />
}

<div style="width:100%;height:100%;margin:0px,0px,0px,0px;padding:0px,0px,0px,0px" ng-app="jbapp" ng-controller="jbctl">

    <div class="main-left">
        <div class="module1">
            <div class="module title">生产看板</div>
        </div>
        <div class="module1">
            <div class="module title">
                <div style="text-align:center">{{shiftid}}</div>
                <div style="text-align:center">{{teamid}}</div>
            </div>
        </div>
        <div class="module1">
            <div class="module title-left1">
                <div id="monthproduct" style="text-align:center">当月产量：5422(箱)</div>
                <div id="mp_h"></div>
            </div>
        </div>
        <div class="module1">
            <div class="module title-left1">
                <div style="text-align:center">{{teamdayproduct}}</div>
                <div id="tdp_h"></div>
            </div>
        </div>
        <div class="module1">
            <div class="module title-left2">
                <div style="align-content:center">{{times_t}}</div>
                <div style="align-content:center">{{times_w}}</div>
                <div style="align-content:center">{{times_n}}</div>
            </div>
        </div>
    </div>

    <div class="main-right">
        <div class="module2">
            <div class="module2-top">
                <div>当班机台日产量</div>
            </div>
            <div class="module2-down">
                <div id="tedp">ccc</div>
            </div>
        </div>

        <div class="module2">
            <div class="module2-top">
                <div>当月牌号产量</div>
            </div>
            <div class="module2-down">
                <div id="mmp">ddd</div>
            </div>
        </div>
    </div>


</div>


@section scripts{
    @Scripts.Render("~/bundles/chart");
    <script src="~/Areas/M/Scripts/JBCJ_Product1.js"></script>
}
