@{
    Layout = "~/Areas/M/Views/Shared/_Layout.cshtml";
}
@section styles{
    <link href="~/Areas/M/Styles/JBConsumptionModel.css" rel="stylesheet" />
}
<div ng-app="XHAPP" ng-controller="XHCTRL" id="main" style="width:100%;height:100%;margin:0px;padding:0px">
    <div class="main-left">
        <div class="module1 title">
            <div style="display:table;width:100%;height:100%;">
                <div style="display:table-cell;vertical-align:middle;width:100%;">
                    <div class="row">
                        <div class="col-xs-6">
                            @ViewBag.Title
                        </div>
                        <div class="col-xs-6">
                            <div id="ProducteWeek" class="week">@ViewBag.Week</div>
                            <div id="datetimes" class="date">{{clock}}</div>
                            <div id="ProducteDate" class="date" style="display: none">@ViewBag.Date</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="module2 bg-color-1">
            <div class="module2-body" style="text-align: center; vertical-align: middle">
                <div class="row">
                    <p style="font-size: 24px;">
                        盘纸
                    </p>
                    (米/万支)
                </div>
                <div class="row">
                    <img src="~/Areas/M/Images/制丝生产.png" width="150" />
                </div>
            </div>
        </div>
        <div class="module2 bg-color-2">
            <div class="module2-body" style="text-align: center; vertical-align: middle">
                <div class="row">
                    <p style="font-size: 24px;">
                        嘴棒
                    </p>
                    (支/万支)
                </div>
                <div class="row">
                    <img src="~/Areas/M/Images/制丝生产.png" width="150" />
                </div>
            </div>
        </div>
        <div class="module2 bg-color-3">
            <div class="module2-body" style="text-align: center; vertical-align: middle">
                <div class="row">
                    <p style="font-size: 24px;">
                        小盒
                    </p>
                    (张/万支)
                </div>
                <div class="row">
                    <img src="~/Areas/M/Images/制丝生产.png" width="150" />
                </div>
            </div>
        </div>
        <div class="module2 bg-color-4">
            <div class="module2-body" style="text-align: center; vertical-align: middle">
                <div class="row">
                    <p style="font-size: 24px;">
                        条盒
                    </p>
                    (张/万支)
                </div>
                <div class="row">
                    <img src="~/Areas/M/Images/制丝生产.png" width="150" />
                </div>
            </div>
        </div>
    </div>

    <div class="header-content exist-left">
        <div class="module3 title" style="background-color:#4D4D4D">
            <div style="display:table;width:100%;height:100%;">
                <div style="display:table-cell;vertical-align:bottom;text-align:right;width:100%;">
                    <div class="row" style="display:none">
                        <form class="form-horizontal">
                            <div class="form-group" style="margin:0px 0px 0px 0px;font-size:small;">

                                <ul class=" list-inline list-inline-extents">
                                    <li></li>
                                    <li ng-click="QueryData(1)" ng-class="{typeatcive: 1 == SelectType}">
                                        <a href="#">当日</a>（
                                    </li>
                                    <li ng-click="QueryData(4)" ng-class="{typeatcive: 4 == SelectType}">
                                        <a href="#">早班</a>|
                                    </li>
                                    <li ng-click="QueryData(5)" ng-class="{typeatcive: 5 == SelectType}">
                                        <a href="#">中班</a>|
                                    </li>
                                    <li ng-click="QueryData(6)" ng-class="{typeatcive: 6 == SelectType}">
                                        <a href="#">夜班</a>）/
                                    </li>
                                    <li ng-click="QueryData(2)" ng-class="{typeatcive: 2 == SelectType}">
                                        <a href="#">当周</a> /
                                    </li>
                                    <li ng-click="QueryData(3)" ng-class="{typeatcive: 3 == SelectType}">
                                        <a href="#">当月</a>
                                    </li>
                                </ul>
                            </div>
                        </form>
                    </div>

                    <div class="row" style="text-align: left; font-size: 24px;color: #4595ec; ">
                        <table>
                            <tr>
                                <td>
                                    本月
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div id="datatimetip" style="text-align: right; font-size: 12px; color: lightblue;"></div>
                </div>
            </div>
        </div>
    </div>

