@{
    Layout = "~/Areas/M/Views/Shared/_Layout.cshtml";
}

@section styles{
    <link href="~/Areas/M/Styles/JBConsumption.css" rel="stylesheet" />
}
<div ng-app="XHAPP" ng-controller="XHCTRL" id="main" style="width:100%;height:100%;margin:0px;padding:0px">
    <div class="main-left">
        <div class="module1 title">
            <div style="display:table;width:100%;height:100%;">
                <div style="display:table-cell;vertical-align:middle;width:100%;">
                    <div class="row">
                        <div class="col-xs-6">
                            @ViewBag.Title
                        </div>
                        <div class="col-xs-6">
                            <div id="ProducteWeek" class="week">@ViewBag.Week</div>
                            <div id="datetimes" class="date">{{clock}}</div>
                            <div id="ProducteDate" class="date" style="display: none">@ViewBag.Date</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="bg-color-1" style="height:22.5%">
            <table style="height:100%;width:40%">
                <tr style="text-align:center">
                    <td rowspan="2" width="95%">
                        <p style="font-size:24px">卷烟纸</p>
                        (米/万支)
                    </td>
                    <td width="5%">
                        <div class="col-xs-6" style="font-size: 12px;text-align:right;">
                            前五
                        </div>
                    </td>
                </tr>
                <tr style="text-align: center">
                    <td width="5%">
                        <div class="col-xs-6" style="font-size: 12px;">
                            后五
                        </div>
                    </td>
                </tr>
            </table>
        </div>
        <div class="bg-color-2" style="height:22.5%">
            <table style="height:100%;width:40%">
                <tr style="text-align:center">
                    <td rowspan="2" width="95%">
                        <p style="font-size:24px">嘴棒</p>
                        (支/万支)
                    </td>
                    <td width="5%">
                        <div class="col-xs-6" style="font-size: 12px;text-align:right;">
                            前五
                        </div>
                    </td>
                </tr>
                <tr style="text-align: center">
                    <td width="5%">
                        <div class="col-xs-6" style="font-size: 12px;">
                            后五
                        </div>
                    </td>
                </tr>
            </table>
        </div>
        <div class="bg-color-3" style="height:22.5%">
            <table style="height:100%;width:40%">
                <tr style="text-align:center">
                    <td rowspan="2" width="95%">
                        <p style="font-size:24px">小盒</p>
                        (张/万支)
                    </td>
                    <td width="5%">
                        <div class="col-xs-6" style="font-size: 12px;text-align:right;">
                            前五
                        </div>
                    </td>
                </tr>
                <tr style="text-align: center">
                    <td width="5%">
                        <div class="col-xs-6" style="font-size: 12px;">
                            后五
                        </div>
                    </td>
                </tr>
            </table>
        </div>
        <div class="bg-color-4" style="height:22.5%">
            <table style="height:100%;width:40%">
                <tr style="text-align:center">
                    <td rowspan="2" width="95%">
