<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="renderer" content="webkit">
    <meta name="viewport" content="width=device-width" />  

    <title>@ViewBag.Title - SMES</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")   

    <link href="~/Areas/ProduceTrace/Style/Site.css" rel="stylesheet" />

    @RenderSection("styles", required: false)

</head>
<body>
    <div>
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery", "~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/smesjs")
    <script src="~/Areas/ProduceTrace/Js/jquery.tmpl.js"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
