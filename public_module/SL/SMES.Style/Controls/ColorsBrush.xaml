<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--**********************************高亮背景色Begin*******************************-->

    
    
    <!--控件基色-->
    <SolidColorBrush x:Key="SmesColorControlBaseBrush" Color="#FF2F3034"/>   <!--#FF4595EC-->
    <!--控件基色深度+1-->
    <SolidColorBrush x:Key="SmesColorControlBasePlus1Brush" Color="#FF1E1E21"/>
    <!--控件背景基色（淡灰色）-->
    <SolidColorBrush x:Key="SmesColorControlBaseBgBrush" Color="#FFFFFFFF"/>
    <!--控件前景基色（）-->
    <SolidColorBrush x:Key="SmesColorControlForgroundBaseBrush" Color="#FF2F3034"/>
    
    <SolidColorBrush x:Key="SmesColorControlForgroundBaseWhiteBrush" Color="#FFFFFFFF"/>

    <SolidColorBrush x:Key="SmesColorControlBaseGreen" Color="#FF6BB06B"/>
    <SolidColorBrush x:Key="SmesColorControlBaseGreenPlus" Color="#FF74CD74"/>

    <!--禁用色-->
    <SolidColorBrush x:Key="SmesColorControlDisabledBrush" Color="#FFA9ACA9"/>

    <!--**********************************高亮背景色End*******************************-->
</ResourceDictionary>
